    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true,
        "quoteProps": "as-needed",
        "trailingComma": "es5",
        "endOfLine": "lf",
        "arrowParens": "always"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
